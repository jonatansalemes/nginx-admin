			<artifactId>nginx-admin-agent-client</artifactId>
			<version>${project.version}</version>
		</dependency>
		<dependency>
			<groupId>com.jslsolucoes</groupId>
			<artifactId>tagria-lib</artifactId>
			<version>1.0.15</version>
		</dependency>
		<dependency>
			<groupId>com.jslsolucoes</groupId>
